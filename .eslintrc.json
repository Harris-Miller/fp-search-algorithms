{
  "extends": [
    "harris/base"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "parserOptions": {
        "project": [
          "tsconfig.lib.json",
          "tsconfig.spec.json"
        ]
      },
      "rules": {
        "no-continue": "off",
        "@typescript-eslint/no-non-null-assertion": "off"
      }
    },
    {
      "files": [
        "*.test.ts"
      ],
      "rules": {
        "no-console": "off"
      }
    }
  ]
}
